## --*- sh -*--

export EF_PROTECT_BELOW=1
export EF_PROTECT_FREE=1
export EF_FILL=42

datadir=${srcdir}/testsuite/data
basefile=`basename $0`
basefile=${datadir}/${basefile%%.sh}

REPLACE_REGEX='s!^.*\(: (Re)reading blacklist\)!TIME\1!;
               s![\t ]\([0-9]:\)!0\1!g;
	       :a s!:\([0-9]:\)!:0\1!g;ta;
	       s!:\([0-9]\)$!:0\1!g;
	       s!\(de:ad:be:ef:00:\)[^[:blank:]]*!\1XX!g'

function exists()
{
    which "$1" >/dev/null 2>&1
}

set -e
